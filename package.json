{
  "name": "divorce-prototype",
  "description": "GOVUK prototyping app in Express",
  "version": "2.1.0",
  "private": true,
  "engines": {
    "node": "^6.10.1"
  },
  "scripts": {
    "start": "NODE_PATH=. node start.js",
    "debug": "NODE_PATH=. node-debug server.js",
    "test": "NODE_PATH=. mocha test/unit-tests/ --reporter spec --recursive",
    "test-a11y": "NODE_PATH=. mocha test/accessibility-tests/ --reporter spec --recursive --timeout 15000 || true",
    "test-unit": "NODE_PATH=. mocha test/unit-tests/ --reporter spec --recursive",
    "test-e2e": "NODE_PATH=. codeceptjs run test/e2e-tests/ --steps --reporter spec",
    "lint": "NODE_PATH=. eslint .",
    "setup": "NODE_PATH=. grunt generate-assets",
    "package": "./bin/package.sh divorce-prototype"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.14.1",
    "check-types": "7.0.0",
    "co": "^4.0.0",
    "co-request": "^1.0.0",
    "connect-redis": "3.0.2",
    "consolidate": "0.x",
    "eslint": "^3.5.0",
    "express": "4.13.3",
    "express-nunjucks": "2.0.1",
    "express-session": "1.13.0",
    "express-validator": "git+https://git@github.com/michaeldfallen/express-validator.git#nesting-validations",
    "express-writer": "0.0.4",
    "forever": "^0.15.2",
    "govuk-elements-sass": "~3.0.2",
    "govuk_frontend_toolkit": "~6.0.1",
    "govuk_template_jinja": "https://github.com/alphagov/govuk_template/releases/download/v0.17.0/jinja_govuk_template-0.17.0.tgz",
    "govuk_template_mustache": "^0.17.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-concurrent": "0.4.3",
    "grunt-contrib-watch": "0.5.3",
    "grunt-eslint": "^19.0.0",
    "grunt-nodemon": "0.4.0",
    "grunt-sass": "^2.0.0",
    "grunt-sync": "^0.5.1",
    "hogan.js": "3.0.2",
    "jquery": "^3.2.1",
    "jquery-migrate": "^3.0.0",
    "lodash": "^4.17.11",
    "log4js": "^0.6.38",
    "minimist": "0.0.8",
    "moment": "2.13.0",
    "moment-duration-format": "^2.2.2",
    "node-sass": "^4.0.0",
    "nunjucks": "git+https://git@github.com/gonogo/nunjucks.git",
    "portscanner": "^1.0.0",
    "prompt": "^0.2.14",
    "readdir": "0.0.6",
    "redis": "^2.6.2",
    "request": "^2.81.0",
    "restler": "3.4.0",
    "serve-favicon": "2.3.0",
    "twilio": "^2.11.1",
    "validator": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "eslint-plugin-mocha": "^4.5.1",
    "mocha": "^3.0.0",
    "supertest": "^2.0.0",
    "pa11y": "^4.0.1",
    "codeceptjs": "^0.4.7",
    "nightmare": "^2.7.0",
    "nightmare-upload": "^0.1.1",
    "mochawesome": "^1.5.3"
  }
}
